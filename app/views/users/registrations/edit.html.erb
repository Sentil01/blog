<div class="container mt-4">
  <h2><%= resource_name.to_s.humanize %> Profile</h2>
  <br/>
  <p>User Email: <%= current_user.email %></p>
  <br/>
  <div class="modal fade" id="navModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLabel">Edit User</h5>
          <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="newUserForm">
          <%= render "edit"%>

        </div>


      </div>
    </div>
  </div>'
  <button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#navModal">
    Edit Profile
  </button>
  <br/>
  <br/>


  <p>Unhappy? <%= button_to "Cancel my account", registration_path(resource_name), data: { confirm: "Are you sure?" }, method: :delete %></p>

  <%= link_to "Back", :back %>


</div>